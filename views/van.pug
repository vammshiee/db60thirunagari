extends layout

block content
  h1= title
  - let vans=results
  div(class="container-fluid")
  div(class="row")
    each van,index in vans
      div(class="col-sm-2 " onclick=`buy(${index})`) #{van.Name} - #{van.Company} - #{van.Price} 
  script.
      function buy(itemNo)
      {
        alert(`You purchased Item ${itemNo+1}`)
      }     
    